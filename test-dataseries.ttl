@prefix ex:   <http://example.org/> .

# --- Dataset Series ---

ex:series-covid19-cases a dcat:DatasetSeries ;
    dct:title "COVID-19 Case Reports (Monthly)" ;
    dct:description "A time series of monthly COVID-19 case statistics." ;
    dct:identifier "series-covid19-cases" ;
    dct:publisher ex:public-health-agency ;
    dct:issued "2023-01-01T00:00:00Z"^^xsd:dateTime ;
    dct:modified "2025-06-01T00:00:00Z"^^xsd:dateTime ;
    dcat:first ex:dataset-covid19-2023-01 ;
    dcat:last ex:dataset-covid19-2025-05 ;
    dcat:distribution ex:distribution-covid19-series-overview .

# --- Series-level distribution (optional) ---

ex:distribution-covid19-series-overview a dcat:Distribution ;
    dct:title "COVID-19 Case Series Overview (CSV)" ;
    dct:issued "2023-01-01T00:00:00Z"^^xsd:dateTime ;
    dcat:accessURL <https://example.org/data/covid19/series-overview.csv> .

# --- Publisher agent ---

ex:public-health-agency a foaf:Agent ;
    foaf:name "National Public Health Agency" .

